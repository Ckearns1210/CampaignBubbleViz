	sub @Uppercase @Uppercase germandbls' by Germandbls;
	sub @Uppercase germandbls' space @Uppercase @Uppercase by Germandbls;
} Germandbls;

lookup UppercaseHyphen {
	sub @Uppercase hyphen' @Uppercase by hyphen.case;
} UppercaseHyphen;

#lookup aFillerPrepPrep {
#	sub f' f [f t] @alike by f.a;
#} aFillerPrepPrep;
#
#lookup aFillerPrep {
#	sub f' [f t] @alike by f.a;
#} aFillerPrep;

lookup aFiller {
	sub [t f]' @alike by [t.a f.a];
} aFiller;

#lookup fPrep {
#	sub f' f @Stumper by f.short;
#} fPrep;

lookup fShort {
	sub f' @Stumper by f.short;
} fShort;

lookup RShort {
	sub [R Racute Rcaron Rcommaaccent Rdotbelow Rdotbelowmacron Rlinebelow]' @Kicker by [R.short Racute.short Rcaron.short Rcommaaccent.short  Rdotbelow.short Rdotbelowmacron.short Rlinebelow.short];
} RShort;

lookup ClipAscenders {
	sub [dcaron lcaron] @LeftAscender' by @ClippedAscender;
} ClipAscenders;

lookup jacute {
	sub iacute j.loclNLD' by j.acute;
	sub Iacute J.loclNLD' by J.acute;
} jacute;
} calt;

